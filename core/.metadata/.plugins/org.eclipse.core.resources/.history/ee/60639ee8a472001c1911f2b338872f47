package ex1;
//class
//java have 4 visiblity modifier: public , protected , default , private 
class Part{
	private int id;	//instance varibale
	private String name;
	private double price;
	private static String brand;//class variable
	
	//setter , getter, mutator
	public void setId(int tempId) {
		if(id==0)
		id=tempId;
		else
			System.out.println("its already set");
	}
	public int getId() {
		return id;
	}
	//ctr
	//logic methods
	
}
public class DemoArrayOfObjects {

	public static void main(String[] args) {
		Part part=new Part();
		part.setId(3);
		
		System.out.println(part.getId());
		
		
	}
}
