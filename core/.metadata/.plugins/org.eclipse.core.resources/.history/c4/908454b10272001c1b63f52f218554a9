package day1;

public class DemoDate {

	public static void main(String[] args) {
		// isLeapYear
		boolean check=isLeapYear(2001);
		System.out.println(check);
		// daysInMonth

	}

	public static boolean isLeapYear(int year) {
		if ((year % 400 == 0 || year % 100 != 0) && (year % 4 == 0))
			return true;
		else
			return false;

	}
}
