package ex1;
//class
//java have 4 visiblity modifier: public , protected , default , private 
class Part{
	int id;	//instance varibale
	String name;
	double price;
	static String brand;//class variable
	
	
}
public class DemoArrayOfObjects {

	public static void main(String[] args) {
		Part part=new Part();
		System.out.println(part);//ex1.Part@15db9742 ?
		System.out.println(part.id);
		System.out.println(part.name);
		System.out.println(part.price);
		part.id=4;
		System.out.println(part.id);
		
	}
}
