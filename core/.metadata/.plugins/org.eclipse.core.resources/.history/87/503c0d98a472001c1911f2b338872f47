package ex1;
//class
//java have 4 visiblity modifier: public , protected , default , private 
class Part{
	int id;	//instance varibale
	String name;
	double price;
	static String brand;//class variable
	
	
}
public class DemoArrayOfObjects {

	public static void main(String[] args) {
		Part part=new Part();
		
		part.id=4;
		System.out.println(part.id);
		
	}
}
