package ex1;
//class
//java have 4 visiblity modifier: public , protected , default , private 
class Part{
	private int id;	//instance varibale
	private String name;
	private double price;
	private static String brand;//class variable
	
	//setter , getter, mutator
	public void setId(int tempId) {
		id=tempId;
	}
	//ctr
	//logic methods
	
}
public class DemoArrayOfObjects {

	public static void main(String[] args) {
		Part part=new Part();
		
		part.id=4;
		
		
	}
}
