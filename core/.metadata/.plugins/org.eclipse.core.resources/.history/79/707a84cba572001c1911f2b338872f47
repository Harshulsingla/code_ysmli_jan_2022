package ex1;
//class
//java have 4 visiblity modifier: public , protected , default , private 
class Part{
	private int id;	//instance varibale
	private String name;
	private double price;
	private static String brand="ymsli";//class variable
	
	 Part() {
		System.out.println("it a defulat ctr");
	}
	
	 public Part(int tempId, String tempName, double tempPrice)
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getPrice() {
		return price;
	}
	public void setPrice(double price) {
		this.price = price;
	}
	public static String getBrand() {
		return brand;
	}
	public static void setBrand(String brand) {
		Part.brand = brand;
	}
	
	
	
	//setter , getter, mutator

	
	
	//ctr
	//logic methods
	
}
public class DemoArrayOfObjects {

	public static void main(String[] args) {
		Part part=new Part();
		part.setId(3);
		part.setId(44);
		System.out.println(part.getId());
		
		
	}
}
